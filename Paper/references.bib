	keywords = {Humans, Treatment Outcome, Female, Aged, Anti-Bacterial Agents, Adolescent, Adult, Middle Aged, Male, Young Adult, Analysis of Variance, Bone Plates, Intraoperative Care, Bone Wires, Fracture Fixation, Internal, Radius Fractures},
	pages = {g4807},
	file = {Full Text:/Users/smithj/Zotero/storage/H5P57H7P/Costa et al. - 2014 - Percutaneous fixation with Kirschner wires versus .pdf:application/pdf},
